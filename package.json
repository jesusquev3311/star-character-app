{
  "name": "star-character-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,html}\"",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:commit": "git add . && git commit -m \"chore: update release($npm_package_version) \"",
    "changeset:release": "npm run changeset:version && npm run changeset:commit && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@vaadin/router": "^2.0.0",
    "lit": "^3.3.1"
  }
}
